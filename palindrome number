import java.util.*;
class Palindrome
{
public static void main(String[] args)
{
int rem,rev=0,n,x;
Scanner sc=new Scanner(System.in);
System.out.println("enter the number");
n=sc.nextInt();
x=n;
while(n!=0)
{
rem=n%10;
rev=rev*10+rem;
n=n/10;
}
if(x==rev)
{
System.out.println("it is palindrome");
}
else
{
System.out.println("it is Not palindrome");
}
}
}
